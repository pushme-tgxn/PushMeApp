{
    "cli": {
        "version": ">= 2.9.0"
    },
    "build": {
        "develop": {
            "distribution": "internal",
            "developmentClient": true,
            "env": {
                "BACKEND_URL": "https://pushme.tgxn.net",
                "PROJECT_ID": "net.tgxn.pushme.develop"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "BACKEND_URL": "https://pushme.tgxn.net",
                "PROJECT_ID": "net.tgxn.pushme.preview"
            }
        },
        "production": {
            "distribution": "store",
            "env": {
                "BACKEND_URL": "https://pushme.tgxn.net",
                "PROJECT_ID": "net.tgxn.pushme"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
